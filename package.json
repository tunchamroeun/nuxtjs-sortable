{
  "name": "nuxtjs-sortable",
  "version": "0.0.4",
  "description": "Nuxt module for Sortable.js",
  "repository": "https://github.com/tunchamroeun/nuxtjs-sortable.git",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types.d.mts",
      "import": "./dist/module.mjs"
    }
  },
  "main": "./dist/module.mjs",
  "typesVersions": {
    "*": {
      ".": [
        "./dist/types.d.mts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "nuxt",
    "nuxt-module",
    "vue",
    "vuejs",
    "drag",
    "drop",
    "draggable",
    "sortable",
    "sortablejs",
    "typescript"
  ],
  "scripts": {
    "typecheck": "nuxt typecheck",
    "prepack": "nuxt-module-build build",
    "dev": "pnpm run dev:prepare && nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare playground",
    "release": "pnpm run lint && pnpm run test && pnpm run prepack && changelogen --release && pnpm publish && git push --follow-tags",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "test": "vitest run",
    "test:unit": "vitest run test/unit",
    "test:watch": "vitest watch",
    "test:types": "vue-tsc --noEmit && cd playground && vue-tsc --noEmit"
  },
  "dependencies": {
    "@nuxt/kit": "^4.2.1",
    "sortablejs": "^1.15.3"
  },
  "devDependencies": {
    "@nuxt/devtools": "^3.1.0",
    "@nuxt/eslint": "1.11.0",
    "@nuxt/eslint-config": "^1.10.0",
    "@nuxt/hints": "1.0.0-alpha.2",
    "@nuxt/module-builder": "^1.0.2",
    "@nuxt/schema": "^4.2.1",
    "@nuxt/test-utils": "^3.20.1",
    "@types/node": "latest",
    "@types/sortablejs": "^1.15.8",
    "@vitejs/plugin-vue": "^6.0.2",
    "@vue/test-utils": "^2.4.6",
    "changelogen": "^0.6.2",
    "eslint": "^9.39.1",
    "jsdom": "^26.1.0",
    "nuxt": "^4.2.1",
    "typescript": "~5.9.3",
    "vitest": "^3.2.0",
    "vue-tsc": "^3.1.5"
  }
}
